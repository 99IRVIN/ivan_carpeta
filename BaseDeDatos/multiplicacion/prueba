eval=True
while eval==True:
    numero=int(input('ingrese el numero de la tabla que desee: '))
    if numero==0:
        print('error saliendo del programa: ')
        break
    else:
        for numeros in range(1,13):
            print(numeros,'*',numero,'=',numeros*numero)


